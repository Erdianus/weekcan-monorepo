  },
  "devDependencies": {
    "@hktekno/eslint-config": "workspace:*",
    "@hktekno/prettier-config": "workspace:*",
    "@hktekno/tsconfig": "workspace:*",
    "@types/node": "^20.10.6",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "exports": {
